<Page ux:Class="MapPage">
	<Router ux:Dependency="router" />
	
	<JavaScript File="../R.js"/>
	
	<double ux:Property="Latitude"/>
	<double ux:Property="Longitude"/>
	<NativeViewHost>
		<MapView Latitude="{ReadProperty Latitude}" Longitude="{ReadProperty Longitude}">
		</MapView>
	</NativeViewHost>
</Page>